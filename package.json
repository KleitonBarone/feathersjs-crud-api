{
  "name": "feathersjs-crud-api",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": "Kleiton Barone",
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 24.10.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "sqlite",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "main": "lib/index",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "check": "biome check --write ./src",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.0",
    "@feathersjs/authentication": "^5.0.0",
    "@feathersjs/authentication-client": "^5.0.0",
    "@feathersjs/configuration": "^5.0.0",
    "@feathersjs/errors": "^5.0.0",
    "@feathersjs/feathers": "^5.0.0",
    "@feathersjs/knex": "^5.0.0",
    "@feathersjs/koa": "^5.0.0",
    "@feathersjs/schema": "^5.0.0",
    "@feathersjs/socketio": "^5.0.0",
    "@feathersjs/transport-commons": "^5.0.0",
    "@feathersjs/typebox": "^5.0.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@feathersjs/cli": "^5.0.0",
    "@feathersjs/rest-client": "^5.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^24.10.9",
    "axios": "^1.3.4",
    "cross-env": "^10.1.0",
    "mocha": "^11.7.5",
    "shx": "^0.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "tar": "^7.5.3",
    "diff": "^8.0.3"
  }
}
